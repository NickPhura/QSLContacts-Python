{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["jest"],
  "rules": {
    "no-unused-vars": 1,
    "no-console": 1,
    "semi": 2,
    "experimentalObjectRestSpread": true,
    "react/prop-types": 1,
    "react/display-name": 0,
    "react/no-deprecated": 0
  }
}